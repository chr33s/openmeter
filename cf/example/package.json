{
  "name": "openmeter-customer-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "oxlint --import-plugin --react-perf-plugin --jsx-a11y-plugin src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "7.1.1",
    "react-router-dom": "7.1.1",
    "chart.js": "4.4.7",
    "chartjs-adapter-luxon": "1.3.1",
    "react-chartjs-2": "5.2.0"
  },
  "devDependencies": {
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.20",
    "oxlint": "1.16.0",
    "prettier": "3.4.2",
    "@prettier/plugin-oxc": "0.0.4",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "vite": "6.0.7"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "plugins": ["@prettier/plugin-oxc"],
    "overrides": [
      {
        "files": ["*.ts", "*.tsx"],
        "options": {
          "parser": "oxc"
        }
      }
    ]
  }
}