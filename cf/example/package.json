{
	"name": "openmeter-cf-customer-dashboard",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "prettier --check . && oxlint --import-plugin --react-perf-plugin --node-plugin --promise-plugin --regex-plugin . && tsc --noEmit",
		"dev": "vite dev",
		"fix": "prettier --write . && oxlint --fix --fix-suggestions --import-plugin --node-plugin --promise-plugin --regex-plugin .",
		"preview": "vite preview"
	},
	"imports": {
		"#/*": "./src/*"
	},
	"dependencies": {
		"chart.js": "4.5.0",
		"chartjs-adapter-luxon": "1.3.1",
		"luxon": "3.7.2",
		"react": "19.1.1",
		"react-chartjs-2": "5.3.0",
		"react-dom": "19.1.1",
		"react-router": "7.9.1"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "1.13.2",
		"@prettier/plugin-oxc": "0.0.4",
		"@tailwindcss/vite": "4.1.13",
		"@types/react": "19.1.13",
		"@types/react-dom": "19.1.9",
		"@vitejs/plugin-react": "5.0.3",
		"oxlint": "1.16.0",
		"prettier": "3.6.2",
		"tailwindcss": "4.1.13",
		"typescript": "5.9.2",
		"vite": "7.1.5",
		"wrangler": "4.37.1"
	},
	"prettier": {
		"plugins": [
			"@prettier/plugin-oxc"
		]
	}
}
